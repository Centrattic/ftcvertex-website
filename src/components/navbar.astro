---
import mainLogo from "/logo.svg";
import lion from "/lion.svg";
import logoWithText from "/logoWithText.svg";
import instagram from "/instagram.svg";
import MenuBar from "../components/MenuBar";
---

<div
  class="fixed z-50 flex h-5 w-screen flex-wrap items-center justify-center gap-12 py-11 font-display text-lg font-bold"
>
  <MenuBar client:only="react" />
  <a href="/">
    <div class="relative h-9 w-9">
      <img src={mainLogo} class="h-full w-full" alt="" />
    </div>
  </a>
  <div class="duration-150 hover:px-5">
    <a href="/">Home</a>
  </div>
  <div class="duration-150 hover:px-5">
    <a href="/ourteam">Our Team</a>
  </div>
  <div class="duration-150 hover:px-5">
    <a href="/achievements">Awards and Highlights</a>
  </div>
  <div class="duration-150 hover:px-5">
    <a href="/posts">Outreach</a>
  </div>
  <div class="duration-150 hover:px-5">
    <a href="/vtxcads">VTX CADs</a>
  </div>
  <a href="https://www.instagram.com/ftcvertex/"
    ><img src={instagram} alt="" class="h-8 w-8 invert" />
  </a>
</div>
